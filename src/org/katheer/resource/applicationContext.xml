<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="acc1" class="org.katheer.bean.Account">
        <property name="accNo" value="B101"/>
        <property name="bank" value="IOB"/>
        <property name="branch" value="Dindigul"/>
        <property name="accName" value="Abdul Katheer"/>
        <property name="balance" value="670000"/>
    </bean>

    <bean id="emp1" class="org.katheer.bean.Employee">
        <property name="name" value="Abdul Katheer"/>
        <property name="eid" value="E101"/>
    </bean>
</beans>